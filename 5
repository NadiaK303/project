v = int(input('Введите выручку: '))
print(v)
i = int(input('Введите издержки: '))
print(i)
if v > i:
    print('прибыль')
    r = (v-i) / v
    print('Рентабельность: ' + str(r))
    s = int(input('Введите численность сотрудников: '))
    print('Прибыль на одного сотрудника: ' + str((v-i)/s))

if v < i:
    print('убыток')
