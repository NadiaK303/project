list1 = ['a', 'b', 'c', 'd', 'e']
if len(list1) % 2 == 0:
    i = 0
    while i < len(list1):
        el = list1[i]
        list1[i] = list1[i+1]
        list1[i+1] = el
        i += 2
else:
    i = 0
    while i < len(list1) - 1:
        el = list1[i]
        list1[i] = list1[i + 1]
        list1[i + 1] = el
        i += 2
print(list1)
